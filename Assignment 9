def find_palindrome(value):
    if len(value) == 0 or len(value) == 1:
        return True
    first = value[0]
    last = value[-1]
    if first == last:
        return find_palindrome(value[1:-1])
    else:
        return False

if __name__ == '__main__':
    value = "racecar"
    print(find_palindrome(value))
